<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Recently Aired Movies</name>
    <match>all</match>
    <rule field="airdate" operator="inthelast">
        <value>14 days</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>
